<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="341.0" prefWidth="403.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="patient.PatientController">
    <children>
        <Label layoutX="62.0" layoutY="65.0" text="Nom :" />
        <TextField fx:id="nomField" layoutX="150.0" layoutY="61.0" promptText="Entrez le nom" />

        <Label layoutX="54.0" layoutY="109.0" text="Prénom :" />
        <TextField fx:id="prenomField" layoutX="150.0" layoutY="105.0" promptText="Entrez le prénom" />

        <Label layoutX="20.0" layoutY="160.0" text="Numéro de Sécurité Sociale :" />
        <TextField fx:id="numSecuField" layoutX="182.0" layoutY="156.0" promptText="Entrez le numéro de SS" />

        <Label layoutX="62.0" layoutY="208.0" text="Mutuelle :" />
        <TextField fx:id="mutuelleField" layoutX="162.0" layoutY="204.0" prefHeight="26.0" prefWidth="170.0" promptText="Entrez le nom de la mutuelle" />

        <Button layoutX="178.0" layoutY="258.0" mnemonicParsing="false" onAction="#enregistrerPatient" text="Enregistrer" />
        <Label fx:id="fiche_patient" layoutX="173.0" layoutY="14.0" prefHeight="18.0" prefWidth="84.0" text="Fiche patient">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
      <Button layoutX="186.0" layoutY="301.0" mnemonicParsing="false" onAction="#OnAnnuler" text="Annuler" />
    </children>
</AnchorPane>
